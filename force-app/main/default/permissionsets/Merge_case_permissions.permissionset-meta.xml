<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Settings needed for user to complete a merge case successfully. read, write, edit and delete on case object.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Merge case permissions</label>
