<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Reassign FormAssembly Leads</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Contractor_Recruitment</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.CreatedById</field>
                <operation>contains</operation>
                <value>Raso</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>equals</operation>
                <value>Contractor Lead</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>brendon.davis@taskeasy.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.OwnerId</field>
                <operation>contains</operation>
                <value>Raso</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>notEqual</operation>
                <value>Contractor Lead</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>brendon.davis@taskeasy.com</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.OwnerId</field>
                <operation>contains</operation>
                <value>Shosted</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
